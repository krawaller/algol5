{
  "name": "algol-games",
  "scripts": {
    "robot": "npm install; npm run export;",
    "export": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/export",
    "revise": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/revise",
    "retemplate": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/retemplate",
    "stub": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/stub",
    "analyze": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/analyze.ts",
    "fake": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/fake",
    "watch": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/watch",
    "validate": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/games/commands/validate",
    "test": "cd ../..; npx jest modules/games/test",
