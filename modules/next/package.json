{
  "scripts": {
    "robot": "npm run getStatic && npm run makeGamePages && npm run build && npm run export",
    "clean": "rm -rf dist; mkdir dist; rm -rf .next; rm -rf public;",
    "makeGamePages": "cd ../..; npx babel-node --extensions '.ts','.js' ./modules/next/commands/makeGamePages",
    "getStatic": "rm -rf ./public; cp -r ../ui/dist/static ./public",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export -o dist",
    "deploy": "./commands/deploy.sh"
  },
  "dependencies": {
    "next": "^9.1.6"
  },
  "devDependencies": {
    "svgexport": "^0.4.0"
  }
}
